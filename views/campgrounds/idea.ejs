<% include ../partials/header %>
<div class="row">
  <h1 style="text-align: center;">Submit a Project idea</h1>
  <div class="rwd-form">
    <form action="/idea" method="POST" enctype="multipart/form-data">
      <div class="form-group">
        <label for="name">Name</label>
        <input class="form-control" type="text" name="name" placeholder="Kiran" autocomplete="off" required />
      </div>
     
      <div class="form-group">
        <label for="price">Phone</label>
        <input class="form-control" type="number" name="Phone" placeholder="+91" autocomplete="off" required />
      </div>
      <div class="form-group">
        <label for="location">Year</label>
        <input class="form-control" type="number" name="year" id="year" placeholder="I" autocomplete="off" />
      </div>
      <div class="form-group">
        <label for="description">PIN</label>
        <input class="form-control" type="number" name="PIN" placeholder=" 22153....."  height="100" autocomplete="off" />
      </div>
      <div class="form-group">
        <label for="description">Idea</label>
        <input class="form-control" type="text" name="idea" placeholder=" I have an Idea ....."  height="100" autocomplete="off" />
      </div>
      <div class="form-group">
        <button class="btn btn-md btn-primary btn-block">Submit!</button>
      </div>
    </form>
    
    <a href="/campgrounds">Back</a>
  </div>
</div>
<% include ../partials/footer %>